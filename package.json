    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "chokidar": "^2.0.3",
    "nodemon": "^1.17.5",
    "source-map-support": "^0.5.6",
    "tape": "^4.9.1"
  }
